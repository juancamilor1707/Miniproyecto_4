<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.miniproyecto4.controller.WinController">
    <padding>
        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
    </padding>

    <Label text="¡VICTORIA!" style="-fx-font-size: 48px; -fx-font-weight: bold; -fx-text-fill: #27AE60;" />

    <Label fx:id="statsLabel" style="-fx-font-size: 20px;" />

    <Button fx:id="newGameButton" text="Nuevo Juego" prefWidth="200.0" style="-fx-font-size: 16px;" />

    <Button fx:id="menuButton" text="Menú Principal" prefWidth="200.0" style="-fx-font-size: 16px;" />
</VBox>