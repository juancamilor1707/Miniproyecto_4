<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="615.0" prefWidth="630.0" styleClass="menu" stylesheets="@Css/Styles.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.miniproyecto4.controller.MenuController">

    <TextField fx:id="nicknameField" maxWidth="200.0" promptText="Nickname" style="-fx-font-size: 14px;">
      <font>
         <Font name="Schoolbell Regular" size="12.0" />
      </font>
      <VBox.margin>
         <Insets top="170.0" />
      </VBox.margin></TextField>
   <Button fx:id="newGameButton" mnemonicParsing="false" prefHeight="95.0" prefWidth="110.0" styleClass="button1" text="Nuevo Juego" />

    <Button fx:id="continueButton" prefWidth="200.0" style="-fx-font-size: 16px;" styleClass="button2" stylesheets="@Css/Styles.css" text="Continuar" />

    <Button fx:id="helpButton" prefWidth="200.0" style="-fx-font-size: 16px;" styleClass="button2" stylesheets="@Css/Styles.css" text="Ayuda" />

    <Button fx:id="exitButton" prefWidth="200.0" style="-fx-font-size: 16px;" styleClass="button1" stylesheets="@Css/Styles.css" text="Salir" />
</VBox>
